app_debug = false[common]prefix = common_prefix[app]default_timezone = Asia/Shanghai[cache]driver = fileexpire = 0token_select = 1[cookie]expire = 1440[database]driver = mysqltype = mysqlhostname = database_hostnamedatabase = database_databaseusername = database_usernamepassword = database_passwordhostport = database_hostportcharset = utf8mb4prefix = database_prefixtrigger_sql = false[lang]default_lang = zh-cn[log]channel = filemax_files = 20[redis]type = redishost = redis_hostport = redis_portpassword = redis_password[sission]type = fileexpire = 1440